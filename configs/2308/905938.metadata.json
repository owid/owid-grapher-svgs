  "id": 905938,
  "name": "Employment in agriculture, female (% of female employment) (modeled ILO estimate)",
  "unit": "modeled ILO estimate",
  "createdAt": "2024-05-31T06:52:26.000Z",
  "updatedAt": "2024-09-30T18:42:25.000Z",
  "coverage": "",
  "timespan": "1991-2022",
  "datasetId": 6543,
