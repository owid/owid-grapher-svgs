{
  "id": 905345,
  "name": "Fixed broadband subscriptions (per 100 people)",
  "unit": "per 100 people",
  "createdAt": "2024-05-31T06:51:34.000Z",
  "updatedAt": "2024-07-08T16:50:34.000Z",
  "coverage": "",
  "timespan": "1998-2022",
